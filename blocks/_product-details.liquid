

{%- assign block_settings = block.settings -%}

<div
  id="ProductInformation-{{ section.id }}"
  class="
    product-details
    {% if block_settings.sticky_details_desktop %} sticky-content--desktop{% endif %}
    {% if block_settings.height == 'fill' -%} full-height--desktop{% endif -%}
  "
  style="--details-position: {{ block_settings.details_position }};"
  data-testid="product-information-details"
  {% if settings.transition_to_main_product %}
    data-view-transition-type="product-details"
  {% endif %}
>
  {% capture children %}
    <div class="view-product-title">
      <a
        href="{{ product.selected_or_first_available_variant.url }}"
        class="link"
      >
        {{- product.title }}
      </a>
    </div>
    <div class="product-details-header">
      <div class="product-details-links">
        <span>
          <a href="/">Home</a>
          <span>></span>
          <a href="/collections/all">KNIVES</a>
          <span>></span>
          <span>{{ product.title }}</span>
        </span>
        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 16 16" fill="none">
          <g clip-path="url(#clip0_4592_48340)">
            <path d="M14.8537 7.35375L9.85371 12.3537C9.78378 12.4238 9.69466 12.4714 9.59761 12.4908C9.50057 12.5101 9.39998 12.5002 9.30857 12.4623C9.21716 12.4244 9.13904 12.3603 9.0841 12.278C9.02917 12.1957 8.99988 12.0989 8.99996 12V9.51437C5.43121 9.71687 2.98434 12.0312 2.29746 12.7644C2.18961 12.8796 2.04817 12.9578 1.89328 12.9879C1.73838 13.0181 1.57793 12.9986 1.43475 12.9323C1.29157 12.8659 1.17295 12.7561 1.09579 12.6185C1.01863 12.4809 0.986842 12.3224 1.00496 12.1656C1.23684 10.1494 2.34121 8.21 4.11496 6.705C5.58809 5.455 7.37871 4.66187 8.99996 4.52312V2C8.99988 1.90105 9.02917 1.8043 9.0841 1.72201C9.13904 1.63971 9.21716 1.57556 9.30857 1.53769C9.39998 1.49981 9.50057 1.48991 9.59761 1.50923C9.69466 1.52856 9.78378 1.57624 9.85371 1.64625L14.8537 6.64625C14.9002 6.69268 14.9371 6.74783 14.9622 6.80853C14.9874 6.86923 15.0004 6.93429 15.0004 7C15.0004 7.0657 14.9874 7.13077 14.9622 7.19147C14.9371 7.25217 14.9002 7.30731 14.8537 7.35375Z" fill="#86868B"/>
          </g>
          <defs>
            <clipPath id="clip0_4592_48340">
              <rect width="16" height="16" fill="white"/>
            </clipPath>
          </defs>
        </svg>
      </div>
      <h2 id="product-variant-title">{{ product.title }} — {{ product.selected_or_first_available_variant.title }}</h2>
      <span>MODEL: {{ product.metafields.custom.model }}</span>
    </div>
    {% comment %} <div class="product-reviews"></div> {% endcomment %}
    <div class="product-details-prices">
      <h2>{{ product.price | money }}</h2>
      {% if product.price < product.compare_at_price %}
        <h5>{{ product.compare_at_price | money }}</h5>
      {% endif %}
    </div>
    <div class="product-details-shipping">
      <div class="product-shipping-header">
        <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 20 20" fill="none">
          <g clip-path="url(#clip0_4592_48393)">
            <path d="M10 15C10.5178 15 10.9375 14.5803 10.9375 14.0625C10.9375 13.5447 10.5178 13.125 10 13.125C9.48223 13.125 9.0625 13.5447 9.0625 14.0625C9.0625 14.5803 9.48223 15 10 15Z" fill="#C1C4C7"/>
            <path d="M10 11.25V10.625C11.3805 10.625 12.5 9.64531 12.5 8.4375C12.5 7.22969 11.3805 6.25 10 6.25C8.61953 6.25 7.5 7.22969 7.5 8.4375V8.75" stroke="#C1C4C7" stroke-width="1.2" stroke-linecap="round" stroke-linejoin="round"/>
            <path d="M10 17.5C14.1421 17.5 17.5 14.1421 17.5 10C17.5 5.85786 14.1421 2.5 10 2.5C5.85786 2.5 2.5 5.85786 2.5 10C2.5 14.1421 5.85786 17.5 10 17.5Z" stroke="#C1C4C7" stroke-width="1.2" stroke-linecap="round" stroke-linejoin="round"/>
          </g>
          <defs>
            <clipPath id="clip0_4592_48393">
              <rect width="20" height="20" fill="white"/>
            </clipPath>
          </defs>
        </svg>
        <span>Earliest Delivery <b>Tomorrow</b> to
          <span>
            {{ section.settings.shipping_title2 }}
            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 16 16" fill="none">
              <g clip-path="url(#clip0_4592_48402)">
                <path d="M13 6L8 11L3 6" stroke="#86868B" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
              </g>
              <defs>
                <clipPath id="clip0_4592_48402">
                  <rect width="16" height="16" fill="white"/>
                </clipPath>
              </defs>
            </svg>
          </span>
        </span>
      </div>
      <div class="product-shipping-content">
        <div>
          <img src="{{ section.settings.shipping_icon1 | img_url: 'master' }}" width="100%" height="100%" alt="Shipping Icon1">
          <span>{{ section.settings.shipping_content1 }}</span>
        </div>
        <div>
          <img src="{{ section.settings.shipping_icon2 | img_url: 'master' }}" width="100%" height="100%" alt="Shipping Icon2">
          <span>{{ section.settings.shipping_content2 }}</span>
        </div>
      </div>
    </div>
    <div class="product-details-info">
      <span>{{ section.settings.detail_content }}</span>
      <div class="product-detail_info-divs">
        <div class="product-detail_info-div">
          <img src="{{ section.settings.detail_icon1 | img_url: 'master' }}" width="100%" height="100%" alt="Detail Icon1">
          <span>{{ section.settings.detail_info1 }}</span>
        </div>
        <div class="product-detail_info-div">
          <img src="{{ section.settings.detail_icon2 | img_url: 'master' }}" width="100%" height="100%" alt="Detail Icon2">
          <span>{{ section.settings.detail_info2 }}</span>
        </div>
        <div class="product-detail_info-div">
          <img src="{{ section.settings.detail_icon3 | img_url: 'master' }}" width="100%" height="100%" alt="Detail Icon3">
          <span>{{ section.settings.detail_info3 }}</span>
        </div>
      </div>
    </div>
    {% content_for 'blocks' %}
    <div class="product-details-contact">
      <h4>{{ section.settings.contact_heading }}</h4>
      <div class="product-detail_info-divs">
        <div class="product-detail_info-div">
          <img src="{{ section.settings.contact_icon1 | img_url: 'master' }}" width="100%" height="100%" alt="Contact Icon1">
          <span>{{ section.settings.contact_info1 }}</span>
        </div>
        <div class="product-detail_info-div">
          <img src="{{ section.settings.contact_icon2 | img_url: 'master' }}" width="100%" height="100%" alt="Contact Icon2">
          <span>{{ section.settings.contact_info2 }}</span>
        </div>
        <div class="product-detail_info-div">
          <img src="{{ section.settings.contact_icon3 | img_url: 'master' }}" width="100%" height="100%" alt="Contact Icon3">
          <span>{{ section.settings.contact_info3 }}</span>
        </div>
      </div>
    </div>
  {% endcapture %}

  {% render 'group', children: children, settings: block_settings, shopify_attributes: block.shopify_attributes %}
</div>

{% style %}
  .product-details span {
    font-family: "Sen";
    font-weight: 400;
    font-size: 14px;
    color: #86868b;
  }

  .product-details h2 {
    font-family: "Sen";
    font-weight: 600;
    font-size: 36px;
    line-height: 135%;
    color: #f9f9f7;
    margin: 0;
  }

  .product-details p {
    font-family: "Albert Sans";
    font-weight: 400;
    font-size: 16px;
    margin: 0;
  }

  .product-details h5 {
    font-family: "Sen";
    font-weight: 600;
    font-size: 22px;
    color: #86868b;
    text-decoration-line: line-through;
  }

  .product-details h4 {
    font-family: "Sen";
    font-weight: 600;
    font-size: 16px;
    color: #f9f9f7;
  }

  .product-details-prices {
    display: flex;
    align-items: flex-end;
    gap: 12px;
  }
  
  .product-details-links {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 16px;
  }

  .product-details-links > span {
    display: flex;
    gap: 6px;
  }

  .product-details-links a {
    color: #86868b;
  }

  .product-details-header h2 {
    margin-bottom: 12px;
  }

  .product-details-prices h4 {
    font-family: "Sen";
    font-weight: 600;
    font-size: 22px;
    margin-bottom: 7px;
  }

  .product-details-shipping {
    display: flex;
    flex-direction: column;
    gap: 20px;
  }

  .product-shipping-header {
    display: flex;
    gap: 12px;
    align-items: center;
  }

  .product-shipping-header > span {
    display: flex;
    align-items: center;
    gap: 5px;
  }

  .product-shipping-header > span > b {
    color: #f9f9f7;
    font-weight: 500;
  }

  .product-shipping-header > span strong {
    font-weight: 500;
    color: #f9f9f7;
  }

  .product-shipping-header span span {
    display: flex;
    align-items: center;
    gap: 6px;
    padding: 4px 0;
    border-bottom: 1px solid 
  }

  .product-shipping-content img {
    width: 20px;
    height: 20px;
  }

  .product-shipping-content {
    padding: 20px 16px;
    display: flex;
    flex-direction: column;
    gap: 12px;
    border-radius: 8px;
    border: 1px solid var(--Stroke, rgba(255, 255, 255, 0.1));
    background: rgba(255, 255, 255, 0.03);
  }

  .product-shipping-content div {
    display: flex;
    align-items: center;
    gap: 16px;
  }

  .product-details-info {
    display: flex;
    flex-direction: column;
    gap: 16px;
  }

  .product-details-info > span {
    font-size: 16px;
  }

  .product-detail_info-divs {
    display: flex;
    gap: 16px;
  }

  .product-detail_info-div {
    width: calc(100% / 3 - 32px / 3);
    text-align: center;
  }

  .product-detail_info-div img {
    width: 64px;
    height: 64px;
    margin: auto;
  }

  .product-variants {
    display: flex;
    flex-wrap: wrap;
    gap: 16px;
  }

  .product-variant {
    width: 100%;
  }

  .product-variant.active {
    border: 4px solid #86868b;
    box-shadow: 0 1px 2px 0 rgba(0, 0, 0, 0.25);
  }

  .product-variant.active img {
    padding: 8px;
  }

  .product-variant span {
    display: block;
    margin: 12px 8px;
    overflow: hidden;
  }

  .product-variant.active span {
    color: #f9f9f7;
  }

  @media screen and (max-width: 750px) {
    .product-detail_info-div img {
      width: 40px;
      height: 40px;
    }

    .product-details h2 {
      font-size: 22px;
    }
  }

  .product-detail_info-div strong {
    font-weight: 400;
    color: #c1c4c7;
  }

  .product-detail_info-div p {
    font-family: "Sen";
    font-weight: 400;
    font-size: 14px;
    color: #86868b;
  }

  .product-detail p {
    font-family: "Albert Sans";
    font-weight: 400;
    font-size: 16px;
    color: #86868b;
  }

  .product-details .add-to-cart-button {
    background-color: #fd3e36;
    color: white;
  }

  .product-details .add-to-cart-button span {
    color: white;
  }

  .product-details accordion-custom .details__header {
    padding: 12px 0;
    font-family: "Sen";
    font-weight: 400;
    font-size: 16px;
    color #f9f9f7;
  }

  .product-details .accordion p px {} {
    margin: 0;
  }

  .product-details  .accordion-custom .details-content p {
    font-family: "Albert Sans";
    font-weight: 600;
    font-size: 12x; 
    color: #86868b;
  }

  .product-details p em {

  }

  .product-details-contact {
    margin-top: 20px;
  }
{% endstyle %}

{% stylesheet %}
  /* Clear padding on mobile, if not full-width */
  @media screen and (max-width: 749px) {
    .product-information.section--page-width .product-details > .group-block {
      padding-inline: 0;
    }
  }

  .view-product-title {
    display: none;
  }

  /* Container styles */
  .product-details {
    display: flex;
    align-self: start;
    justify-content: center;
  }

  @media screen and (min-width: 750px) {
    .product-details > .group-block {
      height: min-content;
    }

    .full-height--desktop {
      height: 100%;
      max-height: calc(100vh - var(--header-group-height, 0));
      min-height: fit-content;
    }

    .full-height--desktop .group-block {
      align-self: var(--details-position, 'flex-start');
    }
  }
{% endstylesheet %}

