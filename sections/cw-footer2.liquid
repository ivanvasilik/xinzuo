{% style %}
    .section-{{ section.id }} footer {
        padding: 0 64px;
        background: linear-gradient(180deg, #1A1A22 0%, #23242C 100%);
    }

    .section-{{ section.id }} .footer-menu {
        display: flex;
        justify-content: space-between;
        padding: 48px 0;
        border-top: 1px solid rgba(255, 255, 255, 0.05);
        border-bottom: 1px solid rgba(255, 255, 255, 0.05);
    }

    .section-{{ section.id }} .footer-menu .footer-left {
        padding-right: 32px;
        max-width: 360px;
    }

    .section-{{ section.id }} .footer-menu .footer-left img {
        margin-bottom: 32px;
    }

    .section-{{ section.id }} .footer-menu .footer-left p {
        font-family: "Albert Sans";
        font-weight: 400;
        font-size: 16px;
        color: #86868b;
    }

    .section-{{ section.id }} .footer-menu .footer-left p:nth-child(even) {
        font-weight: 600;
    }

    .section-{{ section.id }} .footer-menu .footer-right {
        display: flex;
        gap: 100px;
        width: 702px;
    }

    .section-{{ section.id }} .footer-menu .footer-right .footer-right-menus {
        display: flex;
        gap: 100px;
    }

    .section-{{ section.id }} .menu__item.paragraph a {
        display: block;
        font-family: "Albert Sans";
        font-weight: 400;
        font-size: 16px;
        color: #f9f9f7;
        margin-top: 20px;
    }

    .section-{{ section.id }} .footer-list .menu-item {
        display: flex;
        gap: 16px;
        align-items: center;
        margin-top: 20px;
        width: max-content;
    }

    .section-{{ section.id }} .footer-list .menu-item img {
        width: 20px;
        height: 20px;
    }

    .section-{{ section.id }} .footer-list .menu-item p {
        font-family: "Albert Sans";
        font-weight: 400;
        font-size: 16px;
        color: #f9f9f7;
    }

    .section-{{ section.id }} .footer-list .social-links {
        display: flex;
        gap: 21px;
        list-style: none;
        padding: 0;
        margin-top: 20px;
    }

    /* .section-{{ section.id }} .footer-list .social-links  */

    @media screen and (max-width: 1025px) {
        .section-{{ section.id }} .footer-menu .footer-right {
            gap: 30px;
        }

        .section-{{ section.id }} .footer-menu .footer-right .footer-right-menus {
            gap: 30px;
        }
    }

    @media screen and (max-width: 750px) {
        .section-{{ section.id }} footer {
            padding: 0 32px;
        }

        .section-{{ section.id }} .footer-menu {
            display: block;
            padding: 32px 0;
        }

        .section-{{ section.id }} .footer-left {
            padding: 0 !important;
            width: 100%;
            text-align: center;
            margin: 0 auto 32px;
        }

        .section-{{ section.id }} .footer-right {
            display: block !important;
            width: auto !important;
        }

        .section-{{ section.id }} .footer-right-menus {
            margin-bottom: 48px;
        }

        .section-{{ section.id }} .footer-list {
            display: flex;
            flex-direction: column;
            align-items: center;
        }

        .section-{{ section.id }} .footer-list .menu-title {
            margin: 0;
        }

        .section-{{ section.id }} .footer-list .menu-item {
            margin-top: 16px;
        }

        .section-{{ section.id }} .menu__item.paragraph a {
            margin-top: 12px;
        }

        .section-{{ section.id }} .footer-list .social-links {
            margin-top: 16px;
            margin-bottom: 0;
        }

        .section-{{ section.id }} .footer-menu .footer-left img {
            width: 200px;
            text-align: center;
            margin: 0 auto 32px;
        }
    }
{% endstyle %}

