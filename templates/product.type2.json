    "product_info_mmnV94": {
      "type": "product-info",
      "blocks": {
        "icon_7jadN9": {
          "type": "icon",
          "settings": {
            "image": "shopify://shop_images/Square_3.png",
            "content": "<p>Carbon (~1.0%) boosts <strong>hardness and sharpness</strong>, giving knives superior edge retention and precision performance compared to standard stainless steels.</p>"
          }
        },
        "icon_mxe7iw": {
          "type": "icon",
          "settings": {
            "image": "shopify://shop_images/Square_4.png",
            "content": "<p>Cobalt (~1.5%) <strong>strengthens the steel’s structure, stabilizing hardness</strong> and enhancing performance under heavy professional use.</p>"
          }
        },
        "icon_NxVM3T": {
          "type": "icon",
          "settings": {
            "image": "shopify://shop_images/Square_1.png",
            "content": "<p>Molybdenum increases <strong>toughness and prevents brittleness</strong>, helping the blade resist chipping during demanding kitchen work.</p>"
          }
        },
        "icon_GWBWPm": {
          "type": "icon",
          "settings": {
            "image": "shopify://shop_images/Square_2.png",
            "content": "<p>Vanadium <strong>refines the steel’s grain structure</strong>, producing sharper, harder edges that remain effective longer between regular sharpening.</p>"
          }
        },
        "block_UJiBwA": {
          "type": "block",
          "settings": {
            "header": "With ~1.0% Carbon, sharpness up to",
            "content": "2× longer",
            "footer": "than German stainless steels"
          }
        },
        "block_XD86y8": {
          "type": "block",
          "settings": {
            "header": "With ~1.5% Cobalt, for heavy use, delivers",
            "content": "1.5× stability",
            "footer": "than standard stainless steels"
          }
        },
        "block_pXdtFM": {
          "type": "block",
          "settings": {
            "header": "With Vanadium refinement,",
            "content": "50% longer edge",
            "footer": "lasting precision between sharpenings."
          }
        }
      },
      "block_order": [
        "icon_7jadN9",
        "icon_mxe7iw",
        "icon_NxVM3T",
        "icon_GWBWPm",
        "block_UJiBwA",
        "block_XD86y8",
        "block_pXdtFM"
      ],
      "name": "Product Info",
      "settings": {
        "image": "shopify://shop_images/product-pm8s-003_2x_a3d0418e-6c2d-45f2-84dc-7e9a4d2be98f.png",
        "heading": "",
        "content": "<p><strong>10Cr15CoMoV (VG10-equivalent) </strong>combines high carbon for edge retention, cobalt for stability, chromium for corrosion resistance, and vanadium for sharpness. This balanced composition makes Retro Series ideal for chefs seeking long-lasting sharpness, durability, and artistry in their knives.</p>",
        "first_num": "2",
        "second_num": "3",
        "color_scheme": "scheme-5",
        "padding_top": 80,
        "padding_bottom": 80
      }
    },
    "product_info2_7JTdmJ": {
      "type": "product-info2",
      "blocks": {
        "block_GUqr8N": {
          "type": "block",
          "settings": {
            "heading": "Core Strength",
            "content": "<p>At the core, 10Cr15CoMoV steel provides hardness, sharpness, and lasting precision for demanding professional kitchen use.</p>"
          }
        },
        "block_C7wDEE": {
          "type": "block",
          "settings": {
            "heading": "Protective Layers",
            "content": "<p>Two SUS410 stainless steel layers provide resilience, corrosion resistance, and stability, protecting the premium core.</p>"
          }
        }
      },
      "block_order": [
        "block_GUqr8N",
        "block_C7wDEE"
      ],
      "name": "Product Info2",
      "settings": {
        "heading_title": "TOUGH. BALANCED. RELIABLE",
        "heading_content": "<h2>3 Layer Composite</h2>",
        "header_type": "left",
        "block_type": "text",
        "heading_footer": "Engineered with a VG10-equivalent core and stainless layers, ensuring stability, sharpness, and durability for every kitchen.",
