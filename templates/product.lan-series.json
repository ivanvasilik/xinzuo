    "product_info_mmnV94": {
      "type": "product-info",
      "blocks": {
        "icon_7jadN9": {
          "type": "icon",
          "settings": {
            "image": "shopify://shop_images/Square_3.png",
            "content": "<p><strong>Carbon Hardness</strong><br>This is the \"stays sharp\" secret. The high-carbon core holds a razor edge for months, not weeks.</p>"
          }
        },
        "icon_mxe7iw": {
          "type": "icon",
          "settings": {
            "image": "shopify://shop_images/Square.png",
            "content": "<p><strong>Chromium Protection</strong><br>This is your defense against rust and stains. It keeps the blade clean and hassle-free, even in a busy, wet kitchen.</p>"
          }
        },
        "icon_NxVM3T": {
          "type": "icon",
          "settings": {
            "image": "shopify://shop_images/Square_1.png",
            "content": "<p><strong>Molybdenum Toughness</strong><br>This is the \"anti-chip\" ingredient. It adds serious toughness so your blade isn't brittle. All the performance, none of the panic.</p>"
          }
        },
        "icon_GWBWPm": {
          "type": "icon",
          "settings": {
            "image": "shopify://shop_images/Square_2.png",
            "content": "<p><strong>Vanadium Sharpness</strong><br>This helps create a finer, tighter steel structure. In human terms? It lets the blade take a truly surgical edge and hold it.</p>"
          }
        },
        "icon_4HNknW": {
          "type": "icon",
          "settings": {
            "image": "shopify://shop_images/Square_4.png",
            "content": "<p><strong>Cobalt Stability</strong><br>This is the pro-level support crew. It adds serious strength and wear-resistance, keeping the blade stable under heavy use.</p>"
          }
        },
        "icon_QbUatt": {
          "type": "icon",
          "settings": {
            "image": "shopify://shop_images/Square_32275f87-7708-4c45-9407-e6c3fcd0f2a5.png",
            "content": "<p><strong>Niobium Performance</strong><br>This is the flagship's secret weapon. It lets us push the hardness to the extreme for incredible edge retention, but keeps the blade tough enough for real kitchen work.</p>"
          }
        },
        "block_UJiBwA": {
          "type": "block",
          "settings": {
            "header": "At 62-64 HRC",
            "content": "Flagship Edge Retention",
            "footer": "This is our hardest steel, holding an edge far longer than conventional blades."
          }
        },
        "block_XD86y8": {
          "type": "block",
          "settings": {
            "header": "14Cr14MoVNb Powder Steel",
            "content": "Ultra-Fine Performance",
            "footer": "The microscopic grain structure takes a surgical edge for the cleanest cuts."
          }
        },
        "block_pXdtFM": {
          "type": "block",
          "settings": {
            "header": "Niobium & Vanadium Core",
            "content": "Toughness Meets Hardness",
            "footer": "The blade is hard enough for extreme sharpness, but tough enough to resist chips."
          }
        },
        "block_4FCBML": {
          "type": "block",
          "settings": {
            "header": "73-Layer Damascus",
            "content": "Engineered Protection",
            "footer": "The core is protected by 73 layers of steel, ensuring durability and corrosion resistance."
          }
        }
      },
