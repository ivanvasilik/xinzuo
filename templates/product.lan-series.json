      "name": "Product Info",
      "settings": {
        "width": "page",
        "image": "shopify://shop_images/image_128_b35a7974-cbcd-440f-bfdf-e777b9df4fba.png",
        "header_type": "left",
        "heading": "<h2>Every element chosen with purpose</h2>",
        "content": "<p>This is our flagship steel for a reason. Powder metallurgy creates an incredibly fine, dense blade structure that <strong>takes a surgical edge and holds it longer than anything else we make.</strong> </p>",
        "icon_block_state": true,
        "first_num": "2",
        "second_num": "2",
        "color_scheme": "scheme-5",
        "padding_top": 80,
        "padding_bottom": 80
