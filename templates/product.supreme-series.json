    "product_info2_8biKrj": {
      "type": "product-info2",
      "name": "Product Info2",
      "settings": {
        "width": "page",
        "heading_title": "THAT \"GLIDE-THROUGH\" FEELING",
        "heading_content": "<h2>Sharp Enough for Tomatoes. Tough Enough for Butternut.</h2>",
        "header_type": "left",
        "background_type": "none",
        "block_type": "text",
        "heading_footer": "That 15° edge is the difference between a clean, effortless slice and a mushy mess. This is prep work made fun.",
        "image_video": "image",
        "img": "shopify://shop_images/1.png",
        "color_scheme": "scheme-5",
        "footer_state": "visible",
        "footer_type": "first",
        "footer_left": "<h2>YOUR RELIABLE WORKHORSE</h2>",
        "footer_right": "<p>A lot of 'pro' knives are secretly fragile. This one is different. It's a true workhorse, built from tough German 1.4116 steel. It handles daily prep, resists stains, and sharpens up in seconds. All the performance, none of the panic.</p>",
        "padding_top": 80,
        "padding_bottom": 80,
        "additional_footer": false,
        "additional_content1": "",
        "additional_content2": ""
      }
    },
    "product_info3_Gx89LP": {
      "type": "product-info3",
      "name": "Product Info3",
      "settings": {
        "width": "page",
        "heading": "Built to Perform",
        "img": "shopify://shop_images/product-x02-004.png",
        "img_text": "Full Tang for Perfect Balance",
        "info_header1": "A Breeze to Maintain",
        "info_content1": "~56-58 HRC",
        "info_footer1": "This is the workhorse sweet spot. It's hard enough to hold a great edge, but tough enough to resist chipping and—best of all—a total breeze to re-sharpen at home.",
        "info_header2": "Effortless, Clean Cuts",
        "info_content2": "~15° Edge Cut",
        "info_footer2": "A 15° edge glides through food with less effort, giving you cleaner, smoother, and more professional-looking cuts. It just works.",
        "color_scheme": "scheme-5"
      }
    },
    "product_info2_eVwTgB": {
      "type": "product-info2",
      "disabled": true,
      "name": "Product Info2",
      "settings": {
        "width": "full",
        "heading_title": "Handle Design",
        "heading_content": "<h2>Elegance You<br/>Can Hold</h2>",
        "header_type": "center",
        "background_type": "none",
        "block_type": "text",
        "heading_footer": "",
        "image_video": "image",
        "img": "shopify://shop_images/Image.png",
        "color_scheme": "scheme-5",
        "footer_state": "visible",
        "footer_type": "second",
        "footer_left": "<p>The Supreme Series handle is made from hand-polished redwood, shaped to fit comfortably in your grip. Its ergonomic design reduces fatigue, while the natural warmth of wood adds elegance and balance to every slice. A handle that feels as refined as it looks.</p>",
        "footer_right": "<h2>Ergonomic</h2><p>Comfortable grip for effortless control.</p><h2>Polished</h2><p>Smooth surface with premium feel.</p><h2>Warmth</h2><p>Wooden handle brings inviting touch.</p>",
