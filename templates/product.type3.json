    "product_info_mmnV94": {
      "type": "product-info",
      "blocks": {
        "icon_7jadN9": {
          "type": "icon",
          "settings": {
            "image": "shopify://shop_images/Square_3.png",
            "content": "<p>Carbon (~1.0%) delivers <strong>hardness and  edge sharpness</strong>, ensuring knives’ blades perform with precision and retain their edge through daily professional use.</p>"
          }
        },
        "icon_mxe7iw": {
          "type": "icon",
          "settings": {
            "image": "shopify://shop_images/Square.png",
            "content": "<p>Chromium (~15%) <strong>forms a protective surface layer</strong>, giving the blade strong resistance to rust, stains, and everyday kitchen moisture exposure.</p>"
          }
        },
        "icon_NxVM3T": {
          "type": "icon",
          "settings": {
            "image": "shopify://shop_images/Square_1.png",
            "content": "<p>Molybdenum boosts <strong>toughness and minimizes brittleness</strong>, allowing the blade to withstand heavy chopping and repeated use without risk of cracking or chipping.</p>"
          }
        },
        "icon_GWBWPm": {
          "type": "icon",
          "settings": {
            "image": "shopify://shop_images/Square_2.png",
            "content": "<p>Vanadium <strong>creates a fine-grained steel structure</strong>, producing sharper edges and extending cutting performance longer between sharpening sessions.</p>"
          }
        },
        "icon_4HNknW": {
          "type": "icon",
          "settings": {
            "image": "shopify://shop_images/Square_4.png",
            "content": "<p>Cobalt (~1.5%) <strong>stabilizes hardness throughout the blade</strong>, enhancing wear resistance and consistent strength during intense, continuous cooking tasks.</p>"
          }
        },
        "block_UJiBwA": {
          "type": "block",
          "settings": {
